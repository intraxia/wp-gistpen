<div data-brk-container="editor" class="wpgp-editor">
    <div class="wpgp-editor-row">
        {{> component/editor/description/index}}
    </div>

    <div class="wpgp-editor-row">
        {{> component/editor/controls/index}}
    </div>

    {{#each editor.instances as |instance|}}
        <div class="wpgp-editor-row">
            {{> component/editor/instance/index instance=instance}}
        </div>
    {{/each}}
</div>
