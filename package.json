  },
  "wp-env": {
    "plugin-dir": "wp-gistpen",
    "plugin-name": "WP-Gistpen",
    "docker-template": "./bin/docker-compose.override.yml.template",
    "welcome-build-command": "npm run dev:app"
  }
}
