{
  "name": "wp-gistpen",
  "description": "A self-hosted alternative to putting your code snippets on Gist.",
  "author": {
    "name": "James DiGioia",
    "email": "jamesorodig@gmail.com",
    "twitter": "@JamesDiGioia"
  },
  "homepage": "http://www.jamesdigioia.com/wp-gistpen/",
  "license": "GPL-2.0+",
  "scripts": {
    "test": "npm run test:typecheck && npm run test:lint && npm run test:unit",
    "test:typecheck": "tsc --noEmit",
    "test:lint": "eslint \"client/**/*.{json,ts,tsx}\" && prettier --check \"client/**/*.{json,ts,tsx}\"",
    "test:unit": "npm run test:unit:ts && npm run test:unit:codemod",
    "test:unit:ts": "beaver test --env test",
    "test:unit:codemod": "mocha --reporter spec --ui bdd --colors --timeout 10000 codemods/__tests__/*.spec.js",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "beaver build --env development",
    "build:prod": "beaver build --env production",
    "dev:app": "beaver build --env development --watch",
    "dev:tdd": "beaver test --env test --watch",
    "dev:storybook": "start-storybook -s ./assets/js -p 9001 -c .storybook",
    "dev:format": "prettier --write \"client/**/*.{json,ts,tsx}\" && eslint \"client/**/*.{json,ts,tsx}\" --fix",
    "gen": "npm run gen:coc && npm run gen:pot",
    "gen:coc": "weallbehave -o .",
    "gen:pot": "wp-pot --bug-report http://github.com/intraxia/wp-gistpen --domain wp-gistpen --package wp-gistpen --src 'app/**/*.php' --relative-to . --dest-file languages/wp-gistpen.pot"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.6.2",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "@testing-library/react": "^8.0.9",
    "@types/chai": "^4.2.3",
    "@types/classnames": "^2.2.9",
    "@types/kefir": "^3.8.2",
    "@types/lolex": "^3.1.1",
    "@types/mocha": "^5.2.7",
    "@types/react-dom": "^16.9.1",
    "@types/react-redux": "^7.1.4",
    "@types/recompose": "^0.30.7",
    "@types/sinon": "^7.5.0",
    "@types/sprintf-js": "^1.1.2",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__react": "^4.0.2",
    "@types/tinymce": "^4.5.23",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "acorn": "6.2.1",
    "babel-preset-brookjs": "1.0.0-alpha.12",
    "brookjs-cli": "1.0.0-alpha.23",
    "brookjs-desalinate": "1.0.0-alpha.23",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-valtech": "^1.0.0",
    "eslint-plugin-react": "^7.14.3",
    "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
    "jscodeshift": "^0.6.4",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react-dev-utils": "^9.0.4",
    "sass-loader": "^7.3.1",
    "shelljs": "^0.8.3",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.3.0",
    "style-loader": "^0.23.1",
    "stylelint": "^11.0.0",
    "stylelint-webpack-plugin": "^0.10.1",
    "symbol-observable": "^1.2.0",
    "typescript": "^3.5.3",
    "weallbehave": "^1.2.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-notifier": "^1.8.0",
    "wp-pot-cli": "^1.2.4"
  },
  "dependencies": {
