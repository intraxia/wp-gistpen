{
  "name": "wp-gistpen",
  "description": "A self-hosted alternative to putting your code snippets on Gist.",
  "author": {
    "name": "James DiGioia",
    "email": "jamesorodig@gmail.com",
    "twitter": "@JamesDiGioia"
  },
  "homepage": "http://www.jamesdigioia.com/wp-gistpen/",
  "license": "GPL-2.0+",
  "scripts": {
    "test": "npm run test:typecheck && npm run test:lint && npm run test:unit",
    "test:typecheck": "tsc --noEmit",
    "test:lint": "eslint \"client/**/*.{json,ts,tsx}\" && prettier --check \"client/**/*.{json,ts,tsx}\"",
    "test:unit": "beaver test unit",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "beaver build --env development",
    "build:prod": "beaver build --env production",
    "dev:app": "beaver build --env development --watch",
    "dev:tdd": "beaver test unit --watch",
    "dev:storybook": "start-storybook -s ./assets/js -p 9001 -c .storybook",
    "dev:format": "prettier --write \"client/**/*.{json,ts,tsx}\" && eslint \"client/**/*.{json,ts,tsx}\" --fix",
    "env:install": "wp-scripts env install",
    "env:connect": "wp-scripts env connect",
    "env:start": "wp-scripts env start",
    "env:stop": "wp-scripts env stop",
    "gen": "npm run gen:coc && npm run gen:pot",
    "gen:coc": "weallbehave -o .",
    "gen:pot": "wp-pot --bug-report http://github.com/intraxia/wp-gistpen --domain wp-gistpen --package wp-gistpen --src 'app/**/*.php' --relative-to . --dest-file languages/wp-gistpen.pot"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.7.2",
    "@babel/register": "^7.7.0",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@testing-library/react": "^8.0.9",
    "@types/classnames": "^2.2.9",
    "@types/clipboard": "^2.0.1",
    "@types/jest": "^24.0.22",
    "@types/kefir": "^3.8.2",
    "@types/lolex": "^3.1.1",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/recompose": "^0.30.7",
    "@types/sinon": "^7.5.0",
    "@types/sprintf-js": "^1.1.2",
    "@types/tinymce": "^4.5.23",
    "@types/webpack-env": "^1.14.1",
    "@types/wordpress__blocks": "^6.4.3",
    "@types/wordpress__components": "^7.4.4",
    "@types/wordpress__compose": "^3.4.0",
    "@types/wordpress__element": "^2.4.0",
    "@types/wordpress__i18n": "^3.4.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "@wordpress/scripts": "^5.1.0",
    "acorn": "6.2.1",
    "babel-preset-brookjs": "1.0.0-alpha.12",
    "brookjs-cli": "1.0.0-alpha.25",
    "brookjs-desalinate": "1.0.0-alpha.25",
    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-valtech": "^1.0.0",
    "eslint-plugin-react": "^7.16.0",
    "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
    "node-sass": "^4.13.0",
    "prettier": "^1.18.2",
    "react-dev-utils": "^9.1.0",
    "sass-loader": "^7.3.1",
    "shelljs": "^0.8.3",
    "sinon": "^7.5.0",
    "style-loader": "^0.23.1",
    "stylelint": "^11.1.1",
    "stylelint-webpack-plugin": "^1.0.3",
    "symbol-observable": "^1.2.0",
    "typescript": "^3.7.2",
    "weallbehave": "^1.2.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-notifier": "^1.8.0",
    "wp-pot-cli": "^1.2.4"
  },
  "dependencies": {
    "brookjs": "1.0.0-alpha.25",
    "brookjs-flow": "^1.0.0-alpha.24",
    "brookjs-silt": "1.0.0-alpha.25",
    "brookjs-types": "^1.0.0-alpha.24",
    "classnames": "^2.2.5",
    "clipboard": "^2.0.4",
    "io-ts": "^1.10.4",
    "kefir": "^3.8.6",
    "mutationobserver-shim": "^0.3.3",
    "prism-themes": "^1.1.0",
    "prismjs": "1.17.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "sheet-router": "^4.0.0-0",
    "sprintf-js": "^1.1.2",
    "typesafe-actions": "^4.4.2",
    "typescript-nullable": "^0.6.0",
    "whatwg-fetch": "^3.0.0"
  },
  "wp-env": {
    "plugin-dir": "wp-gistpen",
    "plugin-name": "WP-Gistpen",
    "docker-template": "./bin/docker-compose.override.yml.template",
    "welcome-build-command": "npm run dev:app"
  }
}
