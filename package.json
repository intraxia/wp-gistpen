    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-typescript": "^7.7.4",
    "@babel/register": "^7.7.4",
    "@jest/types": "^24.9.0",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/react": "^5.3.17",
    "@testing-library/react": "^8.0.9",
    "@types/classnames": "^2.2.9",
    "@types/clipboard": "^2.0.1",
    "@types/expect-puppeteer": "^4.4.0",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/jest-image-snapshot": "^2.11.0",
    "@types/kefir": "^3.8.2",
    "@types/lolex": "^5.1.0",
    "@types/puppeteer": "^2.0.1",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/recompose": "^0.30.7",
    "@types/sinon": "^7.5.2",
    "@types/sprintf-js": "^1.1.2",
    "@types/tinymce": "^4.5.23",
    "@types/webpack": "^4.41.0",
    "@types/webpack-env": "^1.15.0",
    "@types/wordpress__blocks": "^6.4.3",
    "@types/wordpress__components": "^8.5.2",
    "@types/wordpress__compose": "^3.4.0",
    "@types/wordpress__element": "^2.4.0",
    "@types/wordpress__i18n": "^3.4.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "@wordpress/e2e-test-utils": "^3.0.0",
    "@wordpress/scripts": "^6.0.0",
    "babel-preset-brookjs": "1.0.0-alpha.26",
