  "dependencies": {
    "brookjs": "1.0.0-beta.2",
    "brookjs-flow": "^1.0.0-alpha.26",
    "brookjs-silt": "1.0.0-beta.2",
    "brookjs-types": "^1.0.0-beta.0",
    "classnames": "^2.2.5",
    "clipboard": "^2.0.4",
    "io-ts": "^1.10.4",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "kefir": "^3.8.6",
