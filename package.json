{
  "name": "wp-gistpen",
  "description": "A self-hosted alternative to putting your code snippets on Gist.",
  "author": {
    "name": "James DiGioia",
    "email": "jamesorodig@gmail.com",
    "twitter": "@JamesDiGioia"
  },
  "homepage": "http://www.jamesdigioia.com/wp-gistpen/",
  "license": "GPL-2.0+",
  "scripts": {
    "test": "npm run test:typecheck && npm run test:check && npm run test:lint && npm run test:unit",
    "test:typecheck": "tsc --noEmit",
    "test:check": "beaver test check",
    "test:lint": "beaver test lint",
    "test:unit": "beaver test unit",
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "beaver build --env development",
    "build:prod": "beaver build --env production",
    "dev:app": "beaver build --env development --watch",
    "dev:tdd": "beaver test unit --watch",
    "dev:storybook": "start-storybook -s ./resources/assets -p 9001 -c .storybook",
    "dev:format": "beaver format",
    "env": "wp-scripts env",
    "env:php:format": "npm run env docker-run php composer dev:format",
    "env:php:test": "npm run env docker-run php composer test",
    "env:php:test:lint": "npm run env docker-run php composer test:lint",
    "env:php:test:unit": "npm run env docker-run php composer test:unit",
    "gen": "npm run gen:coc && npm run gen:pot",
    "gen:coc": "weallbehave -o .",
    "gen:pot": "wp-pot --bug-report http://github.com/intraxia/wp-gistpen --domain wp-gistpen --package wp-gistpen --src 'app/**/*.php' --relative-to . --dest-file languages/wp-gistpen.pot"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-storyshots": "^5.3.18",
    "@storybook/react": "^5.3.17",
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@types/classnames": "^2.2.9",
    "@types/clipboard": "^2.0.1",
    "@types/eslint": "^6.8.1",
    "@types/expect-puppeteer": "^4.4.0",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/jest-image-snapshot": "^3.1.0",
    "@types/kefir": "^3.8.4",
    "@types/lolex": "^5.1.0",
    "@types/nise": "^1.4.0",
    "@types/node": "^13.1.8",
    "@types/prismjs": "^1.16.1",
    "@types/puppeteer": "^3.0.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.8",
    "@types/recompose": "^0.30.7",
    "@types/sinon": "^9.0.4",
    "@types/sprintf-js": "^1.1.2",
    "@types/tinymce": "^4.5.23",
    "@types/webpack": "^4.41.13",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/webpack-env": "^1.15.2",
    "@types/wordpress__blocks": "^6.4.8",
    "@types/wordpress__components": "^8.5.3",
    "@types/wordpress__compose": "^3.4.1",
    "@types/wordpress__element": "^2.4.0",
    "@types/wordpress__i18n": "^3.11.0",
    "@wordpress/components": "^9.6.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^2.6.0",
    "@wordpress/e2e-test-utils": "^4.8.0",
    "@wordpress/scripts": "^9.0.0",
    "babel-plugin-prismjs": "^2.0.1",
    "brookjs-cli": "1.0.0-beta.19",
    "brookjs-desalinate": "1.0.0-beta.17",
    "copy-webpack-plugin": "^5.1.1",
    "execa": "^4.0.1",
    "expect-puppeteer": "^4.4.0",
    "ink": "^2.7.1",
    "jest-image-snapshot": "^4.0.2",
    "jest-puppeteer": "^4.4.0",
    "nise": "^4.0.3",
    "node-sass": "^4.14.1",
    "pptr-testing-library": "^0.6.0",
    "puppeteer": "^3.0.4",
    "react-dev-utils": "^10.0.0",
    "react-testing-kit": "^1.1.1",
    "sass-loader": "^8.0.2",
    "sinon": "^9.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.5.0",
    "stylelint-webpack-plugin": "^2.0.0",
    "symbol-observable": "^1.2.0",
    "typescript": "^3.8.3",
    "weallbehave": "^1.2.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-notifier": "^1.8.0",
    "wp-pot-cli": "^1.4.2"
  },
  "dependencies": {
    "brookjs": "1.0.0-beta.17",
    "classnames": "^2.2.5",
    "clipboard": "^2.0.4",
    "io-ts": "^1.10.4",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "kefir": "^3.8.7",
    "kefir-ajax": "^1.0.2",
    "mutationobserver-shim": "^0.3.3",
    "prism-themes": "^1.1.0",
    "prismjs": "1.20.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "sheet-router": "^4.0.0-0",
    "sprintf-js": "^1.1.2",
    "typesafe-actions": "^4.4.2",
    "typescript-nullable": "^0.6.0",
    "whatwg-fetch": "^3.0.0"
  },
  "wp-env": {
    "plugin-dir": "wp-gistpen",
    "plugin-name": "WP-Gistpen",
    "docker-template": "./bin/docker-compose.override.yml.template",
    "welcome-build-command": "npm run dev:app"
  }
}
